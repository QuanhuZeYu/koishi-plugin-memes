{
  "name": "@quanhuzeyu/koishi-plugin-memes",
  "description": "纯ts实现的memes生成，无需部署额外的后端服务",
  "version": "1.0.10-rc.2",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "homepage": "https://github.com/QuanhuZeYu/koishi-plugin-memes",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "pause": "timeout /t 1 /nobreak > nul",
    "test": "echo \"No tests available\"",
    "build": "tsc",
    "allpub": "npm run pubgit && npm run pubnpm",
    "pubgit": "npm run build && yakumo publish --registry=https://npm.pkg.github.com",
    "pubnpm": "npm run build && yakumo publish --registry=https://registry.npmjs.org",
    "clean": "rmdir /s /q lib",
    "cacheclean": "npm cache clean --force"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "meme"
  ],
  "dependencies": {
    "@quanhuzeyu/koishi-plugin-memelib": "^0.1.2-rc.5",
    "axios": "^1.7.5"
  },
  "peerDependencies": {
    "koishi": "^4.17.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuanhuZeYu/koishi-plugin-memes.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "koishi": {
    "description": {
      "zh": "纯ts实现的memes生成，无需部署额外的后端服务"
    }
  }
}
